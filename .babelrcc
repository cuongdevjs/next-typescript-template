{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "fileName": true,
        "pure": true,
        "preprocess": true,
        "namespace": "Panoee"
      }
    ]
  ],
  "ignore": ["node_modules"]
}
