{
  "name": "template-nextjs",
  "version": "2022.07.13.00",
  "private": true,
  "scripts": {
    "dev": "next -- -p 3003",
    "start:dev": "cross-env NODE_ENV=development next dev -- -p 4000",
    "start": "NODE_OPTIONS='-r next-logger' cross-env PORT=8000 next start",
    "build": "cross-env NODE_ENV=production next build",
    "export": "rm -rf .next/ && rm -rf out/ && next build && next export",
    "build_development": "cross-env NODE_ENV=development next build",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "type-check": "tsc --noEmit",
    "next:lint": "next lint",
    "lint": "eslint ./ -c .eslintrc.js --ext ts,tsx --no-error-on-unmatched-pattern --quiet",
    "lint-fix": "eslint ./ -c .eslintrc.js --fix --ext ts,tsx"
  },
  "dependencies": {
